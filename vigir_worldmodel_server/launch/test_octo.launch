<?xml version="1.0"?>

<launch>
  
  <include file="$(find vigir_sim_test_1)/launch/ground_truth_pose_to_tf.launch" />
  
  <node pkg="hector_laserscan_to_pointcloud" type="laserscan_to_pointcloud_node" name="hector_laserscan_to_pointcloud">
    <remap from="scan" to="/multisense_sl/laser/scan" />
  </node>
  
  <include file="$(find vigir_self_filter)/launch/self_filter_lidar.launch" />

  <!--  <node pkg="vigir_worldmodel_main" type="worldmodel_main" name="worldmodel_main" output="screen" launch-prefix="gdb -ex run iiargs">-->
  <node pkg="vigir_worldmodel_main" type="worldmodel_main" name="worldmodel_main" output="screen">
    <remap from="scan_cloud" to="scan_cloud_filtered"/>
    <param name="aggregator_frames" value="/world /pelvis /head" /> 
  </node>
  
</launch>
